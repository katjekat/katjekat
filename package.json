{
  "name": "dat-cha",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "bankai build -j [ --transform babelify --presets [ env ] ] index.js",
    "inspect": "bankai inspect -j [ --transform babelify --presets [ env ] ] index.js",
    "start": "bankai start -j [ --transform babelify --presets [ env ] ] index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "choo": "^6.5.0",
    "choo-devtools": "^1.7.0",
    "choo-service-worker": "^2.3.1",
    "es2040": "^1.2.6",
    "hyperdb": "^1.2.0-rc1",
    "microframe": "^1.0.0",
    "random-access-memory": "^2.4.0",
    "sheetify": "^6.2.0",
    "tachyons": "^4.8.1",
    "budo": "^10.0.4",
    "yo-yoify": "^4.0.0",
    "unassertify": "^2.0.5",
    "bundle-collapser": "^1.3.0",
    "uglifyify": "^4.0.4",
    "npm": "^5.5.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "bankai": "^9.0.0-1",
    "dependency-check": "^2.9.1",
    "standard": "^10.0.3"
  }
}